                                    { 
                                        "type": "frontmost_application_if",
                                        "bundle_identifiers": ["^tv\\.parsec\\.www$"]
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "description": "CMD+Right -> End (end of line)",
                                "from": {
                                    "key_code": "right_arrow",
                                    "modifiers": {
                                        "mandatory": ["command"],
                                        "optional": ["shift"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "end"
                                    }
                                ],
                                "conditions": [
                                    {
                                        "type": "frontmost_application_if",
                                        "bundle_identifiers": ["^tv\\.parsec\\.www$"]
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "description": "CMD+Up -> Page Up",
                                "from": {
                                    "key_code": "up_arrow",
                                    "modifiers": {
                                        "mandatory": ["command"],
                                        "optional": ["shift"]
                                    }
