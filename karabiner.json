                                    {
                                        "type": "frontmost_application_if",
                                        "bundle_identifiers": ["^tv\\.parsec\\.www$"]
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "description": "CMD+Down -> Page Down",
                                "from": {
                                    "key_code": "down_arrow",
                                    "modifiers": {
                                        "mandatory": ["command"],
                                        "optional": ["shift"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "page_down"
                                    }
                                ],
                                "conditions": [
                                    {
                                        "type": "frontmost_application_if",
                                        "bundle_identifiers": ["^tv\\.parsec\\.www$"]
                                    }
                                ]
                            },
                            {
                                "type": "basic",
                                "description": "Ctrl+Minus -> Alt+Left (VSCode navigate back)",
                                "from": {
                                    "key_code": "hyphen",
                                    "modifiers": {
                                        "mandatory": ["control"],
                                        "optional": []
